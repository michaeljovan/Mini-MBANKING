<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="236.0" prefWidth="295.0" spacing="20.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.projectmbanking.Mpayment">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <children>
      <BorderPane prefHeight="150.0" prefWidth="335.0">
         <top>
            <AnchorPane prefHeight="200.0" prefWidth="200.0" style="-fx-background-color: bluE;" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="mpaymentbpjsbutton" layoutX="79.0" layoutY="14.0" mnemonicParsing="false" onAction="#gotompaymentbpjs" text="M-Payment BPJS" />
                  <Button fx:id="mpaymentpublikutilitas" layoutX="51.0" layoutY="160.0" mnemonicParsing="false" onAction="#gotompaymentpublikutilitas" text="M-Payment Publik / Utilitas" />
                  <Button fx:id="mpaymentkartukreditbutton" layoutX="60.0" layoutY="63.0" mnemonicParsing="false" onAction="#gotompaymentkartukredit" text="M-Payment Kartu Kredit" />
                  <Button fx:id="mpaymentinternet" layoutX="71.0" layoutY="109.0" mnemonicParsing="false" onAction="#gotompaymentinternet" text="M-Payment Internet" />
               </children>
            </AnchorPane>
         </top>
         <bottom>
            <AnchorPane prefHeight="200.0" prefWidth="200.0" style="-fx-background-color: BLUE;" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="backbutton" layoutX="182.0" layoutY="160.0" mnemonicParsing="false" onAction="#gotohome" text="Kembali" textFill="RED" />
               </children>
            </AnchorPane>
         </bottom>
      </BorderPane>
   </children>
</VBox>
